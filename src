src/main.js 
import { reactive } from 'vue'
export const store = reactive({
  dadosInvestimento: {}
})
src/App.vue
<template>
  <div class="app">
    <Etapa1 
      v-if="etapaAtual === 1" 
      @proxima="etapaAtual++"  <!-- Evento completo -->
    />
  </div>
</template>

<script setup>
import { ref } from 'vue'
import Etapa1 from './components/Etapa1.vue'  <!-- Nome completo do arquivo -->

const etapaAtual = ref(1)
</script>
src/components/Etapa1.vue
<template>
  <div class="etapa">
    <h2>Etapa 1: Escolha seu Investimento</h2>  <!-- Texto completo -->
    <button @click="$emit('proxima')">  <!-- $emit correto -->
      Pr√≥xima Etapa
    </button>
  </div>
</template>
